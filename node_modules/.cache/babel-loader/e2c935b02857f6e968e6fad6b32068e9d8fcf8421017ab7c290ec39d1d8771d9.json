v class=\"tab-content\" id=\"nav-tabContent\">\n      <div class=\"tab-pane fade show active\" id=\"nav-gamelogs\" role=\"tabpanel\" aria-labelledby=\"nav-gamelogs-tab\">\n        <input v-model=\"seasonFilter\" @input=\"fetchGameLogs\" placeholder=\"Filter by Season\" class=\"form-control mt-3\"/>\n        <table class=\"table table-dark table-striped mt-3\">\n          <thead>\n            <tr>\n              <th>Date</th>\n              <th>Matchup</th>\n              <th>WL</th>\n              <th>MIN</th>\n              <th>FGM</th>\n              <th>FGA</th>\n              <th>FG%</th>\n              <th>3PM</th>\n              <th>3PA</th>\n              <th>3P%</th>\n              <th>FTM</th>\n              <th>FTA</th>\n              <th>FT%</th>\n              <th>REB</th>\n              <th>AST</th>\n              <th>STL</th>\n              <th>BLK</th>\n              <th>PTS</th>\n              <th>+/-</th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr v-for=\"log in gameLogs\" :key=\"log.game_id\">\n              <td>{{ log.game_date }}</td>\n              <td>{{ log.matchup }}</td>\n              <td>{{ log.wl }}</td>\n              <td>{{ log.min }}</td>\n              <td>{{ log.fgm }}</td>\n              <td>{{ log.fga }}</td>\n              <td>{{ log.fg_pct }}</td>\n              <td>{{ log.fg3m }}</td>\n              <td>{{ log.fg3a }}</td>\n              <td>{{ log.fg3_pct }}</td>\n              <td>{{ log.ftm }}</td>\n              <td>{{ log.fta }}</td>\n              <td>{{ log.ft_pct }}</td>\n              <td>{{ log.reb }}</td>\n              <td>{{ log.ast }}</td>\n              <td>{{ log.stl }}</td>\n              <td>{{ log.blk }}</td>\n              <td>{{ log.pts }}</td>\n              <td>{{ log.plus_minus }}</td>\n            </tr>\n          </tbody>\n        </table>\n      </div>\n      <div class=\"tab-pane fade\" id=\"nav-seasonstats\" role=\"tabpanel\" aria-labelledby=\"nav-seasonstats-tab\">\n        <!-- Season Stats Table -->\n      </div>\n      <div class=\"tab-pane fade\" id=\"nav-playoffstats\" role=\"tabpanel\" aria-labelledby=\"nav-playoffstats-tab\">\n        <!-- Playoff Stats Table -->\n      </div>\n    </div>\n  </div>\n  <div v-else>\n    <p>Please select a player to view details</p>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  props: {\n    player: {\n      type: Object,\n      required: false,\n    },\n  },\n  data() {\n    return {\n      gameLogs: [],\n      seasonFilter: '',\n    };\n  },\n  watch: {\n    player: {\n      immediate: true,\n      handler(newPlayer) {\n        if (newPlayer) {\n          this.fetchGameLogs();\n        }\n      },\n    },\n  },\n  methods: {\n    fetchGameLogs() {\n      if (this.player && this.player.id) {\n        axios.get(`/api/players/${this.player.id}/games`, {\n          params: { season: this.seasonFilter },\n        })\n          .then(response => {\n            this.gameLogs = response.data;\n          })\n          .catch(error => {\n            console.error('Error fetching game logs:', error);\n          });\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.player-details {\n  background-color: #2a2a2a;\n  padding: 20px;\n  border-radius: 10px;\n  color: #fff;\n}\n.nav-tabs .nav-link.active {\n  background-color: #007bff;\n  border-color: #007bff;\n}\n.table-dark {\n  color: #fff;\n  background-color: #343a40;\n}\n.table-dark th,\n.table-dark td,\n.table-dark thead th {\n  border-color: #454d55;\n}\n.table-dark.table-striped tbody tr:nth-of-type(odd) {\n  background-color: rgba(255, 255, 255, 0.05);\n}\n</style>\n"],"mappings":"AA4EA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,KAAK,EAAE;IACLC,MAAM,EAAE;MACNC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,KAAK,EAAE;IACLP,MAAM,EAAE;MACNQ,SAAS,EAAE,IAAI;MACfC,OAAOA,CAACC,SAAS,EAAE;QACjB,IAAIA,SAAS,EAAE;UACb,IA