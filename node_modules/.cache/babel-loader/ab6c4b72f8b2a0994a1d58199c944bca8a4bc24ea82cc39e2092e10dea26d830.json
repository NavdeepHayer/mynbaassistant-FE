{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'PlayerSearchPage',\n  data() {\n    return {\n      searchQuery: '',\n      players: [],\n      selectedPlayer: null,\n      gameLogs: [],\n      playoffLogs: [],\n      showDropdown: false\n    };\n  },\n  methods: {\n    handleSearchInput() {\n      if (this.searchQuery.trim() === '') {\n        this.players = [];\n        this.selectedPlayer = null;\n        this.gameLogs = [];\n        this.playoffLogs = [];\n      } else {\n        this.searchPlayers();\n      }\n    },\n    async searchPlayers() {\n      try {\n        const playerResponse = await axios.get(`http://localhost:8000/api/players/${this.searchQuery}`);\n        this.players = playerResponse.data;\n        this.showDropdown = true;\n      } catch (error) {\n        this.players = [];\n        this.showDropdown = false;\n      }\n    },\n    selectPlayer(player) {\n      this.selectedPlayer = player;\n      this.showDropdown = false;\n      this.fetchPlayerData(player.id);\n    },\n    async fetchPlayerData(playerId) {\n      try {\n        const gameLogsResponse = await axios.get(`http://localhost:8000/api/players/${playerId}/games`);\n        this.gameLogs = gameLogsResponse.data;\n        const playoffLogsResponse = await axios.get(`http://localhost:8000/api/players/${playerId}/playoffstats`);\n        this.playoffLogs = playoffLogsResponse.data;\n      } catch (error) {\n        this.gameLogs = [];\n        this.playoffLogs = [];\n      }\n    },\n    hideDropdown() {\n      setTimeout(() => {\n        this.showDropdown = false;\n      }, 200);\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","searchQuery","players","selectedPlayer","gameLogs","playoffLogs","showDropdown","methods","handleSearchInput","trim","searchPlayers","playerResponse","get","error","selectPlayer","player","fetchPlayerData","id","playerId","gameLogsResponse","playoffLogsResponse","hideDropdown","setTimeout"],"sources":["/home/nav/Desktop/NBA api/frontend/nba-stats-app/src/views/SearchPlayerPage.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"player-search mt-4\">\n      <div class=\"input-group mb-3\">\n        <input\n          v-model=\"searchQuery\"\n          @input=\"handleSearchInput\"\n          @keyup.enter=\"searchPlayers\"\n          class=\"form-control\"\n          placeholder=\"Search for a player\"\n          @focus=\"showDropdown = true\"\n          @blur=\"hideDropdown\"\n        />\n        <div class=\"input-group-append\">\n          <button class=\"btn btn-outline-secondary\" @click=\"searchPlayers\">Search</button>\n        </div>\n      </div>\n\n      <div v-if=\"showDropdown && players.length\" class=\"dropdown-menu show w-100\">\n        <button\n          class=\"dropdown-item\"\n          v-for=\"player in players\"\n          :key=\"player.id\"\n          @mousedown=\"selectPlayer(player)\"\n        >\n          {{ player.full_name }}\n        </button>\n      </div>\n\n      <div v-if=\"selectedPlayer\" class=\"mt-4\">\n        <h2>{{ selectedPlayer.full_name }}</h2>\n\n        <div class=\"table-responsive mt-4\">\n          <h3>Regular Season Game Logs</h3>\n          <table class=\"table table-striped\">\n            <thead class=\"thead-dark\">\n              <tr>\n                <th>Date</th>\n                <th>Matchup</th>\n                <th>WL</th>\n                <th>MIN</th>\n                <th>FGM</th>\n                <th>FGA</th>\n                <th>FG%</th>\n                <th>3PM</th>\n                <th>3PA</th>\n                <th>3P%</th>\n                <th>FTM</th>\n                <th>FTA</th>\n                <th>FT%</th>\n                <th>REB</th>\n                <th>AST</th>\n                <th>STL</th>\n                <th>BLK</th>\n                <th>PTS</th>\n                <th>+/-</th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-for=\"log in gameLogs\" :key=\"log.game_id\">\n                <td>{{ log.game_date }}</td>\n                <td>{{ log.matchup }}</td>\n                <td>{{ log.wl }}</td>\n                <td>{{ log.min }}</td>\n                <td>{{ log.fgm }}</td>\n                <td>{{ log.fga }}</td>\n                <td>{{ log.fg_pct }}</td>\n                <td>{{ log.fg3m }}</td>\n                <td>{{ log.fg3a }}</td>\n                <td>{{ log.fg3_pct }}</td>\n                <td>{{ log.ftm }}</td>\n                <td>{{ log.fta }}</td>\n                <td>{{ log.ft_pct }}</td>\n                <td>{{ log.reb }}</td>\n                <td>{{ log.ast }}</td>\n                <td>{{ log.stl }}</td>\n                <td>{{ log.blk }}</td>\n                <td>{{ log.pts }}</td>\n                <td>{{ log.plus_minus }}</td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'PlayerSearchPage',\n  data() {\n    return {\n      searchQuery: '',\n      players: [],\n      selectedPlayer: null,\n      gameLogs: [],\n      playoffLogs: [],\n      showDropdown: false,\n    };\n  },\n  methods: {\n    handleSearchInput() {\n      if (this.searchQuery.trim() === '') {\n        this.players = [];\n        this.selectedPlayer = null;\n        this.gameLogs = [];\n        this.playoffLogs = [];\n      } else {\n        this.searchPlayers();\n      }\n    },\n    async searchPlayers() {\n      try {\n        const playerResponse = await axios.get(\n          `http://localhost:8000/api/players/${this.searchQuery}`\n        );\n        this.players = playerResponse.data;\n        this.showDropdown = true;\n      } catch (error) {\n        this.players = [];\n        this.showDropdown = false;\n      }\n    },\n    selectPlayer(player) {\n      this.selectedPlayer = player;\n      this.showDropdown = false;\n      this.fetchPlayerData(player.id);\n    },\n    async fetchPlayerData(playerId) {\n      try {\n        const gameLogsResponse = await axios.get(\n          `http://localhost:8000/api/players/${playerId}/games`\n        );\n        this.gameLogs = gameLogsResponse.data;\n\n        const playoffLogsResponse = await axios.get(\n          `http://localhost:8000/api/players/${playerId}/playoffstats`\n        );\n        this.playoffLogs = playoffLogsResponse.data;\n      } catch (error) {\n        this.gameLogs = [];\n        this.playoffLogs = [];\n      }\n    },\n    hideDropdown() {\n      setTimeout(() => {\n        this.showDropdown = false;\n      }, 200);\n    },\n  },\n};\n</script>\n\n<style scoped>\n.player-search {\n  position: relative;\n}\n.dropdown-menu {\n  display: block;\n}\n</style>\n"],"mappings":"AAyFA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,kBAAkB;EACxBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,OAAO,EAAE,EAAE;MACXC,cAAc,EAAE,IAAI;MACpBC,QAAQ,EAAE,EAAE;MACZC,WAAW,EAAE,EAAE;MACfC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,iBAAiBA,CAAA,EAAG;MAClB,IAAI,IAAI,CAACP,WAAW,CAACQ,IAAI,CAAC,MAAM,EAAE,EAAE;QAClC,IAAI,CAACP,OAAM,GAAI,EAAE;QACjB,IAAI,CAACC,cAAa,GAAI,IAAI;QAC1B,IAAI,CAACC,QAAO,GAAI,EAAE;QAClB,IAAI,CAACC,WAAU,GAAI,EAAE;MACvB,OAAO;QACL,IAAI,CAACK,aAAa,CAAC,CAAC;MACtB;IACF,CAAC;IACD,MAAMA,aAAaA,CAAA,EAAG;MACpB,IAAI;QACF,MAAMC,cAAa,GAAI,MAAMb,KAAK,CAACc,GAAG,CACpC,qCAAqC,IAAI,CAACX,WAAW,EACvD,CAAC;QACD,IAAI,CAACC,OAAM,GAAIS,cAAc,CAACX,IAAI;QAClC,IAAI,CAACM,YAAW,GAAI,IAAI;MAC1B,EAAE,OAAOO,KAAK,EAAE;QACd,IAAI,CAACX,OAAM,GAAI,EAAE;QACjB,IAAI,CAACI,YAAW,GAAI,KAAK;MAC3B;IACF,CAAC;IACDQ,YAAYA,CAACC,MAAM,EAAE;MACnB,IAAI,CAACZ,cAAa,GAAIY,MAAM;MAC5B,IAAI,CAACT,YAAW,GAAI,KAAK;MACzB,IAAI,CAACU,eAAe,CAACD,MAAM,CAACE,EAAE,CAAC;IACjC,CAAC;IACD,MAAMD,eAAeA,CAACE,QAAQ,EAAE;MAC9B,IAAI;QACF,MAAMC,gBAAe,GAAI,MAAMrB,KAAK,CAACc,GAAG,CACtC,qCAAqCM,QAAQ,QAC/C,CAAC;QACD,IAAI,CAACd,QAAO,GAAIe,gBAAgB,CAACnB,IAAI;QAErC,MAAMoB,mBAAkB,GAAI,MAAMtB,KAAK,CAACc,GAAG,CACzC,qCAAqCM,QAAQ,eAC/C,CAAC;