Math.ceil(this.totalPlayers / this.perPage);\n    }\n  },\n  methods: {\n    fetchPlayers() {\n      axios.get('/api/players')\n        .then(response => {\n          this.players = response.data;\n          this.totalPlayers = this.players.length;\n          if (this.players.length > 0) {\n            this.selectPlayer(this.players[0]);\n          }\n        })\n        .catch(error => {\n          console.error('Error fetching players:', error);\n        });\n    },\n    searchPlayers() {\n      if (this.searchQuery.trim() === '') {\n        this.fetchPlayers();\n      } else {\n        axios.get(`/api/players/${this.searchQuery}`)\n          .then(response => {\n            this.players = response.data;\n            this.totalPlayers = this.players.length;\n            this.page = 1; // Reset to first page\n            if (this.players.length > 0) {\n              this.selectPlayer(this.players[0]);\n            }\n          })\n          .catch(error => {\n            console.error('Error searching players:', error);\n          });\n      }\n    },\n    selectPlayer(player) {\n      this.selectedPlayer = player;\n    },\n    previousPage() {\n      if (this.page > 1) {\n        this.page--;\n      }\n    },\n    nextPage() {\n      if (this.page < this.totalPages) {\n        this.page++;\n      }\n    }\n  },\n  created() {\n    this.fetchPlayers();\n  }\n};\n</script>\n\n<style scoped>\n.players-tab {\n  padding: 15px;\n  background-color: #1a1a1a; /* Match the dark theme */\n  color: #e0e0e0; /* Light text color */\n  border-radius: 8px; /* Rounded corners for modern look */\n}\n.search-box {\n  margin-bottom: 15px;\n}\n.list-group-item {\n  cursor: pointer;\n}\n.list-group-item:hover {\n  background-color: #343a40;\n}\n.pagination-controls {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n</style>\n"],"mappings":"AA0BA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,aAAY,MAAO,qBAAqB;AAE/C,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVF;EACF