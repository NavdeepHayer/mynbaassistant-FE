"tab-pane fade\" id=\"nav-playoffstats\" role=\"tabpanel\" aria-labelledby=\"nav-playoffstats-tab\">\n        <!-- Playoff Stats Content -->\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  props: {\n    player: Object\n  },\n  data() {\n    return {\n      gameLogs: [],\n      seasonFilter: ''\n    };\n  },\n  methods: {\n    fetchGameLogs() {\n      if (this.player) {\n        axios.get(`/api/players/${this.player.id}/games`, {\n          params: {\n            season: this.seasonFilter\n          }\n        })\n        .then(response => {\n          this.gameLogs = response.data;\n        })\n        .catch(error => {\n          console.error('Error fetching game logs:', error);\n        });\n      }\n    }\n  },\n  watch: {\n    player(newPlayer) {\n      if (newPlayer) {\n        this.fetchGameLogs();\n      }\n    }\n  },\n  mounted() {\n    if (this.player) {\n      this.fetchGameLogs();\n    }\n  }\n};\n</script>\n\n<style scoped>\n.player-details {\n  background-color: #1a1a1a;\n  padding: 15px;\n  border-radius: 8px;\n  color: #e0e0e0;\n}\n.nav-tabs .nav-link {\n  color: #e0e0e0;\n}\n.nav-tabs .nav-link.active {\n  background-color: #007bff;\n  border-color: #007bff;\n}\n.table-dark {\n  color: #fff;\n  background-color: #343a40;\n}\n.table-dark th,\n.table-dark td,\n.table-dark thead th {\n  border-color: #454d55;\n}\n.table-dark.table-striped tbody tr:nth-of-type(odd) {\n  background-color: rgba(255, 255, 255, 0.05);\n}\n</style>\n"],"mappings":"AAyEA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,KAAK,EAAE;IACLC,MAAM,EAAEC;EACV,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,aAAaA,CAAA,EAAG;MACd,IAAI,IAAI,CAACN,MAAM,EAAE;QACfF,KAAK,CAACS,GAAG,CAAC,gBAAgB,IAAI,CAACP,MAAM,CAACQ,EAAE,QAAQ,EAAE;UAChDC,MAAM,EAAE;YACNC,MAAM,EAAE,IAAI,CAACN;UACf;QACF,CAAC,EACAO,IAAI,CAACC,QAAO,IAAK;UAChB,IAAI,CAACT,QAAO,GAAIS,QAAQ,CAACV,IAAI;QAC/B,CAAC,EAC