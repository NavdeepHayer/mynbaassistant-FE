{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nexport default {\n  name: 'VerifierCode',\n  data() {\n    return {\n      verifier: '',\n      token: null,\n      leagues: null\n    };\n  },\n  created() {\n    const urlParams = new URLSearchParams(window.location.search);\n    const verifierCode = urlParams.get('code');\n    const token = urlParams.get('token');\n    const leagues = urlParams.get('leagues');\n    if (verifierCode) {\n      this.verifier = verifierCode;\n      this.submitVerifier();\n    }\n    if (token && leagues) {\n      this.token = token;\n      this.leagues = leagues.split(',');\n    }\n  },\n  methods: {\n    async submitVerifier() {\n      try {\n        const response = await fetch('http://api.mynbaassistant.com/auth/yahoo/verifier', {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/x-www-form-urlencoded'\n          },\n          body: new URLSearchParams({\n            verifier: this.verifier\n          })\n        });\n        const data = await response.json();\n        if (data.token) {\n          this.token = data.token;\n          this.leagues = data.leagues;\n\n          // Optionally, store the token in local storage\n          localStorage.setItem('access_token', this.token);\n        } else {\n          alert('Failed to authenticate with Yahoo');\n        }\n      } catch (error) {\n        console.error('Error during Yahoo verifier processing:', error);\n        alert('An error occurred during Yahoo verifier processing');\n      }\n    },\n    goBack() {\n      this.$router.push({\n        name: 'YahooSignIn'\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","verifier","token","leagues","created","urlParams","URLSearchParams","window","location","search","verifierCode","get","submitVerifier","split","methods","response","fetch","method","headers","body","json","localStorage","setItem","alert","error","console","goBack","$router","push"],"sources":["/Users/navdeep/Documents/Github/mynbaassistant-FE/src/views/VerifierCode.vue"],"sourcesContent":["<template>\n    <div class=\"verifier-form container-fluid\">\n      <h1>Yahoo OAuth Verifier</h1>\n      <form @submit.prevent=\"submitVerifier\">\n        <div class=\"form-group\">\n          <label for=\"verifier\">Enter the code from Yahoo:</label>\n          <input type=\"text\" class=\"form-control\" id=\"verifier\" v-model=\"verifier\" required>\n        </div>\n        <button type=\"submit\" class=\"btn btn-primary\">Submit</button>\n      </form>\n      <button class=\"btn btn-secondary mt-3\" @click=\"goBack\">Back</button>\n      <div v-if=\"leagues && leagues.length > 0\">\n        <h2>Your Leagues</h2>\n        <ul>\n          <li v-for=\"league in leagues\" :key=\"league\">{{ league }}</li>\n        </ul>\n      </div>\n      <div v-else-if=\"leagues !== null\">\n        <p>No leagues found or not authenticated yet.</p>\n      </div>\n    </div>\n  </template>\n  \n  <script>\n  export default {\n    name: 'VerifierCode',\n    data() {\n      return {\n        verifier: '',\n        token: null,\n        leagues: null,\n      };\n    },\n    created() {\n      const urlParams = new URLSearchParams(window.location.search);\n      const verifierCode = urlParams.get('code');\n      const token = urlParams.get('token');\n      const leagues = urlParams.get('leagues');\n      if (verifierCode) {\n        this.verifier = verifierCode;\n        this.submitVerifier();\n      }\n      if (token && leagues) {\n        this.token = token;\n        this.leagues = leagues.split(',');\n      }\n    },\n    methods: {\n      async submitVerifier() {\n        try {\n          const response = await fetch('http://api.mynbaassistant.com/auth/yahoo/verifier', {\n            method: 'POST',\n            headers: {\n              'Content-Type': 'application/x-www-form-urlencoded',\n            },\n            body: new URLSearchParams({\n              verifier: this.verifier\n            })\n          });\n          const data = await response.json();\n  \n          if (data.token) {\n            this.token = data.token;\n            this.leagues = data.leagues;\n  \n            // Optionally, store the token in local storage\n            localStorage.setItem('access_token', this.token);\n          } else {\n            alert('Failed to authenticate with Yahoo');\n          }\n        } catch (error) {\n          console.error('Error during Yahoo verifier processing:', error);\n          alert('An error occurred during Yahoo verifier processing');\n        }\n      },\n      goBack() {\n        this.$router.push({ name: 'YahooSignIn' });\n      }\n    }\n  };\n  </script>\n  \n  <style scoped>\n  .verifier-form {\n    width: 100%;\n    padding: 0 15px;\n    background-color: #121212;\n    color: #e0e0e0;\n  }\n  </style>\n  "],"mappings":";;;;AAwBE,eAAe;EACbA,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,IAAI;MACXC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,MAAMC,SAAQ,GAAI,IAAIC,eAAe,CAACC,MAAM,CAACC,QAAQ,CAACC,MAAM,CAAC;IAC7D,MAAMC,YAAW,GAAIL,SAAS,CAACM,GAAG,CAAC,MAAM,CAAC;IAC1C,MAAMT,KAAI,GAAIG,SAAS,CAACM,GAAG,CAAC,OAAO,CAAC;IACpC,MAAMR,OAAM,GAAIE,SAAS,CAACM,GAAG,CAAC,SAAS,CAAC;IACxC,IAAID,YAAY,EAAE;MAChB,IAAI,CAACT,QAAO,GAAIS,YAAY;MAC5B,IAAI,CAACE,cAAc,CAAC,CAAC;IACvB;IACA,IAAIV,KAAI,IAAKC,OAAO,EAAE;MACpB,IAAI,CAACD,KAAI,GAAIA,KAAK;MAClB,IAAI,CAACC,OAAM,GAAIA,OAAO,CAACU,KAAK,CAAC,GAAG,CAAC;IACnC;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMF,cAAcA,CAAA,EAAG;MACrB,IAAI;QACF,MAAMG,QAAO,GAAI,MAAMC,KAAK,CAAC,mDAAmD,EAAE;UAChFC,MAAM,EAAE,MAAM;UACdC,OAAO,EAAE;YACP,cAAc,EAAE;UAClB,CAAC;UACDC,IAAI,EAAE,IAAIb,eAAe,CAAC;YACxBL,QAAQ,EAAE,IAAI,CAACA;UACjB,CAAC;QACH,CAAC,CAAC;QACF,MAAMD,IAAG,GAAI,MAAMe,QAAQ,CAACK,IAAI,CAAC,CAA