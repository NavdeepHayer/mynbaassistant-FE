{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'PlayerSearchPage',\n  data() {\n    return {\n      searchQuery: '',\n      players: [],\n      selectedPlayer: null,\n      gameLogs: [],\n      playoffLogs: [],\n      showDropdown: false\n    };\n  },\n  methods: {\n    async searchPlayers() {\n      try {\n        if (this.searchQuery.trim() === '') {\n          this.players = [];\n          return;\n        }\n        const playerResponse = await axios.get(`http://localhost:8000/api/players/${this.searchQuery}`);\n        this.players = playerResponse.data;\n        this.showDropdown = true;\n      } catch (error) {\n        this.players = [];\n        this.showDropdown = false;\n      }\n    },\n    selectPlayer(player) {\n      this.selectedPlayer = player;\n      this.showDropdown = false;\n      this.fetchPlayerData(player.id);\n    },\n    async fetchPlayerData(playerId) {\n      try {\n        const gameLogsResponse = await axios.get(`http://localhost:8000/api/players/${playerId}/games`);\n        this.gameLogs = gameLogsResponse.data;\n        const playoffLogsResponse = await axios.get(`http://localhost:8000/api/players/${playerId}/playoffstats`);\n        this.playoffLogs = playoffLogsResponse.data;\n      } catch (error) {\n        this.gameLogs = [];\n        this.playoffLogs = [];\n      }\n    },\n    hideDropdown() {\n      setTimeout(() => {\n        this.showDropdown = false;\n      }, 200);\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","searchQuery","players","selectedPlayer","gameLogs","playoffLogs","showDropdown","methods","searchPlayers","trim","playerResponse","get","error","selectPlayer","player","fetchPlayerData","id","playerId","gameLogsResponse","playoffLogsResponse","hideDropdown","setTimeout"],"sources":["/home/nav/Desktop/NBA api/frontend/nba-stats-app/src/views/SearchPlayerPage.vue"],"sourcesContent":["<template>\n  <div class=\"player-search\">\n    <input\n      v-model=\"searchQuery\"\n      @input=\"searchPlayers\"\n      @keyup.enter=\"searchPlayers\"\n      placeholder=\"Search for a player\"\n      @focus=\"showDropdown = true\"\n      @blur=\"hideDropdown\"\n    />\n    <button @click=\"searchPlayers\">Search</button>\n\n    <div v-if=\"showDropdown && players.length\" class=\"dropdown\">\n      <ul>\n        <li v-for=\"player in players\" :key=\"player.id\" @mousedown=\"selectPlayer(player)\">\n          {{ player.full_name }}\n        </li>\n      </ul>\n    </div>\n\n    <div v-if=\"selectedPlayer\">\n      <h2>{{ selectedPlayer.full_name }}</h2>\n\n      <div class=\"table-wrapper\">\n        <h3>Regular Season Game Logs</h3>\n        <table>\n          <thead>\n            <tr>\n              <th>Date</th>\n              <th>Matchup</th>\n              <th>WL</th>\n              <th>MIN</th>\n              <th>FGM</th>\n              <th>FGA</th>\n              <th>FG%</th>\n              <th>3PM</th>\n              <th>3PA</th>\n              <th>3P%</th>\n              <th>FTM</th>\n              <th>FTA</th>\n              <th>FT%</th>\n              <th>REB</th>\n              <th>AST</th>\n              <th>STL</th>\n              <th>BLK</th>\n              <th>PTS</th>\n              <th>+/-</th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr v-for=\"log in gameLogs\" :key=\"log.game_id\">\n              <td>{{ log.game_date }}</td>\n              <td>{{ log.matchup }}</td>\n              <td>{{ log.wl }}</td>\n              <td>{{ log.min }}</td>\n              <td>{{ log.fgm }}</td>\n              <td>{{ log.fga }}</td>\n              <td>{{ log.fg_pct }}</td>\n              <td>{{ log.fg3m }}</td>\n              <td>{{ log.fg3a }}</td>\n              <td>{{ log.fg3_pct }}</td>\n              <td>{{ log.ftm }}</td>\n              <td>{{ log.fta }}</td>\n              <td>{{ log.ft_pct }}</td>\n              <td>{{ log.reb }}</td>\n              <td>{{ log.ast }}</td>\n              <td>{{ log.stl }}</td>\n              <td>{{ log.blk }}</td>\n              <td>{{ log.pts }}</td>\n              <td>{{ log.plus_minus }}</td>\n            </tr>\n          </tbody>\n        </table>\n      </div>\n\n      <div class=\"table-wrapper\">\n        <h3>Playoff Game Logs</h3>\n        <table>\n          <thead>\n            <tr>\n              <th>Date</th>\n              <th>Matchup</th>\n              <th>WL</th>\n              <th>MIN</th>\n              <th>FGM</th>\n              <th>FGA</th>\n              <th>FG%</th>\n              <th>3PM</th>\n              <th>3PA</th>\n              <th>3P%</th>\n              <th>FTM</th>\n              <th>FTA</th>\n              <th>FT%</th>\n              <th>REB</th>\n              <th>AST</th>\n              <th>STL</th>\n              <th>BLK</th>\n              <th>PTS</th>\n              <th>+/-</th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr v-for=\"log in playoffLogs\" :key=\"log.game_id\">\n              <td>{{ log.game_date }}</td>\n              <td>{{ log.matchup }}</td>\n              <td>{{ log.wl }}</td>\n              <td>{{ log.min }}</td>\n              <td>{{ log.fgm }}</td>\n              <td>{{ log.fga }}</td>\n              <td>{{ log.fg_pct }}</td>\n              <td>{{ log.fg3m }}</td>\n              <td>{{ log.fg3a }}</td>\n              <td>{{ log.fg3_pct }}</td>\n              <td>{{ log.ftm }}</td>\n              <td>{{ log.fta }}</td>\n              <td>{{ log.ft_pct }}</td>\n              <td>{{ log.reb }}</td>\n              <td>{{ log.ast }}</td>\n              <td>{{ log.stl }}</td>\n              <td>{{ log.blk }}</td>\n              <td>{{ log.pts }}</td>\n              <td>{{ log.plus_minus }}</td>\n            </tr>\n          </tbody>\n        </table>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'PlayerSearchPage',\n  data() {\n    return {\n      searchQuery: '',\n      players: [],\n      selectedPlayer: null,\n      gameLogs: [],\n      playoffLogs: [],\n      showDropdown: false,\n    };\n  },\n  methods: {\n    async searchPlayers() {\n      try {\n        if (this.searchQuery.trim() === '') {\n          this.players = [];\n          return;\n        }\n        const playerResponse = await axios.get(\n          `http://localhost:8000/api/players/${this.searchQuery}`\n        );\n        this.players = playerResponse.data;\n        this.showDropdown = true;\n      } catch (error) {\n        this.players = [];\n        this.showDropdown = false;\n      }\n    },\n    selectPlayer(player) {\n      this.selectedPlayer = player;\n      this.showDropdown = false;\n      this.fetchPlayerData(player.id);\n    },\n    async fetchPlayerData(playerId) {\n      try {\n        const gameLogsResponse = await axios.get(\n          `http://localhost:8000/api/players/${playerId}/games`\n        );\n        this.gameLogs = gameLogsResponse.data;\n\n        const playoffLogsResponse = await axios.get(\n          `http://localhost:8000/api/players/${playerId}/playoffstats`\n        );\n        this.playoffLogs = playoffLogsResponse.data;\n      } catch (error) {\n        this.gameLogs = [];\n        this.playoffLogs = [];\n      }\n    },\n    hideDropdown() {\n      setTimeout(() => {\n        this.showDropdown = false;\n      }, 200);\n    },\n  },\n};\n</script>\n\n<style scoped>\n.player-search {\n  max-width: 800px;\n  margin: 0 auto;\n  text-align: center;\n  position: relative;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  cursor: pointer;\n  margin: 5px 0;\n  padding: 10px;\n  background-color: #f4f4f4;\n  border: 1px solid #ddd;\n}\nli:hover {\n  background-color: #ddd;\n}\n.dropdown {\n  position: absolute;\n  background-color: white;\n  border: 1px solid #ddd;\n  width: 100%;\n  max-height: 200px;\n  overflow-y: auto;\n  z-index: 1000;\n}\n.table-wrapper {\n  margin-top: 20px;\n  overflow-x: auto;\n}\ntable {\n  width: 100%;\n  b