r);\n      }\n    },\n    async toggleCard(teamId) {\n      if (!this.selectedTeamPlayers[teamId]) {\n        await this.fetchPlayersByTeam(teamId);\n      }\n      this.$set(this.isFlipped, teamId, !this.isFlipped[teamId]);\n    },\n    handleSearchInput() {\n      if (this.searchQuery.trim() === '') {\n        this.players = [];\n      } else {\n        this.searchPlayers();\n      }\n    },\n    async searchPlayers() {\n      try {\n        const playerResponse = await axios.get(`http://localhost:8000/api/players/${this.searchQuery}`);\n        this.players = playerResponse.data;\n        this.showDropdown = true;\n      } catch (error) {\n        this.players = [];\n        this.showDropdown = false;\n      }\n    },\n    async selectPlayer(player) {\n      this.selectedPlayer = player;\n      this.showDropdown = false;\n      await this.fetchPlayerData(player.id);\n    },\n    async fetchPlayerData(playerId) {\n      try {\n        const gameLogsResponse = await axios.get(`http://localhost:8000/api/players/${playerId}/games`);\n        this.gameLogs = gameLogsResponse.data;\n        const seasonStatsResponse = await axios.get(`http://localhost:8000/api/players/${playerId}/seasonstats`);\n        this.seasonStats = seasonStatsResponse.data;\n        const playoffStatsResponse = await axios.get(`http://localhost:8000/api/players/${playerId}/playoffstats`);\n        this.playoffStats = playoffStatsResponse.data;\n      } catch (error) {\n        this.gameLogs = [];\n        this.seasonStats = [];\n        this.playoffStats = [];\n      }\n    },\n    hideDropdown() {\n      setTimeout(() => {\n        this.showDropdown = false;\n      }, 200);\n    }\n  },\n  computed: {\n    filteredGameLogs() {\n      return this.gameLogs.filter(log => log.min != null);\n    },\n    filteredSeasonStats() {\n      return this.seasonStats.filter(stat => stat.min != null);\n    },\n    filteredPlayoffStats() {\n      return this.playoffStats.filter(stat => stat.min != null);\n    }