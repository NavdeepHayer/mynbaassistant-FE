{"ast":null,"code":"import \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nexport default {\n  name: 'VerifierCode',\n  data() {\n    return {\n      verifier: '',\n      token: null,\n      leagues: null\n    };\n  },\n  methods: {\n    async submitVerifier() {\n      try {\n        const response = await fetch('https://your_domain.com/auth/yahoo/verifier', {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/x-www-form-urlencoded'\n          },\n          body: new URLSearchParams({\n            verifier: this.verifier\n          })\n        });\n        const data = await response.json();\n        if (data.token) {\n          this.token = data.token;\n          this.leagues = data.leagues;\n\n          // Optionally, store the token in local storage\n          localStorage.setItem('access_token', this.token);\n        } else {\n          alert('Failed to authenticate with Yahoo');\n        }\n      } catch (error) {\n        console.error('Error during Yahoo verifier processing:', error);\n        alert('An error occurred during Yahoo verifier processing');\n      }\n    }\n  }\n};","map":{"version":3,"names":["name","data","verifier","token","leagues","methods","submitVerifier","response","fetch","method","headers","body","URLSearchParams","json","localStorage","setItem","alert","error","console"],"sources":["/Users/navdeep/Documents/Github/mynbaassistant-FE/src/views/YahooVerifier.vue"],"sourcesContent":["<template>\n    <div class=\"verifier-form container-fluid\">\n      <h1>Yahoo OAuth Verifier</h1>\n      <form @submit.prevent=\"submitVerifier\">\n        <div class=\"form-group\">\n          <label for=\"verifier\">Enter the code from Yahoo:</label>\n          <input type=\"text\" class=\"form-control\" id=\"verifier\" v-model=\"verifier\" required>\n        </div>\n        <button type=\"submit\" class=\"btn btn-primary\">Submit</button>\n      </form>\n      <div v-if=\"leagues && leagues.length > 0\">\n        <h2>Your Leagues</h2>\n        <ul>\n          <li v-for=\"league in leagues\" :key=\"league\">{{ league }}</li>\n        </ul>\n      </div>\n      <div v-else-if=\"leagues !== null\">\n        <p>No leagues found or not authenticated yet.</p>\n      </div>\n    </div>\n  </template>\n  \n  <script>\n  export default {\n    name: 'VerifierCode',\n    data() {\n      return {\n        verifier: '',\n        token: null,\n        leagues: null,\n      };\n    },\n    methods: {\n      async submitVerifier() {\n        try {\n          const response = await fetch('https://your_domain.com/auth/yahoo/verifier', {\n            method: 'POST',\n            headers: {\n              'Content-Type': 'application/x-www-form-urlencoded',\n            },\n            body: new URLSearchParams({\n              verifier: this.verifier\n            })\n          });\n          const data = await response.json();\n  \n          if (data.token) {\n            this.token = data.token;\n            this.leagues = data.leagues;\n  \n            // Optionally, store the token in local storage\n            localStorage.setItem('access_token', this.token);\n          } else {\n            alert('Failed to authenticate with Yahoo');\n          }\n        } catch (error) {\n          console.error('Error during Yahoo verifier processing:', error);\n          alert('An error occurred during Yahoo verifier processing');\n        }\n      }\n    }\n  };\n  </script>\n  \n  <style scoped>\n  .verifier-form {\n    width: 100%;\n    padding: 0 15px;\n    background-color: #121212;\n    color: #e0e0e0;\n  }\n  </style>\n  "],"mappings":";;;AAuBE,eAAe;EACbA,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,IAAI;MACXC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,cAAcA,CAAA,EAAG;MACrB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMC,KAAK,CAAC,6CAA6C,E