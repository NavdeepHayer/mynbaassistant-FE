Logs</h3>\n              <table>\n                <thead>\n                  <tr>\n                    <th>Date</th>\n                    <th>Matchup</th>\n                    <th>WL</th>\n                    <th>MIN</th>\n                    <th>FGM</th>\n                    <th>FGA</th>\n                    <th>FG%</th>\n                    <th>3PM</th>\n                    <th>3PA</th>\n                    <th>3P%</th>\n                    <th>FTM</th>\n                    <th>FTA</th>\n                    <th>FT%</th>\n                    <th>REB</th>\n                    <th>AST</th>\n                    <th>STL</th>\n                    <th>BLK</th>\n                    <th>PTS</th>\n                    <th>+/-</th>\n                  </tr>\n                </thead>\n                <tbody>\n                  <tr v-for=\"log in gameLogs\" :key=\"log.game_id\">\n                    <td>{{ log.game_date }}</td>\n                    <td>{{ log.matchup }}</td>\n                    <td>{{ log.wl }}</td>\n                    <td>{{ log.min }}</td>\n                    <td>{{ log.fgm }}</td>\n                    <td>{{ log.fga }}</td>\n                    <td>{{ log.fg_pct }}</td>\n                    <td>{{ log.fg3m }}</td>\n                    <td>{{ log.fg3a }}</td>\n                    <td>{{ log.fg3_pct }}</td>\n                    <td>{{ log.ftm }}</td>\n                    <td>{{ log.fta }}</td>\n                    <td>{{ log.ft_pct }}</td>\n                    <td>{{ log.reb }}</td>\n                    <td>{{ log.ast }}</td>\n                    <td>{{ log.stl }}</td>\n                    <td>{{ log.blk }}</td>\n                    <td>{{ log.pts }}</td>\n                    <td>{{ log.plus_minus }}</td>\n                  </tr>\n                </tbody>\n              </table>\n            </div>\n\n            <div class=\"table-wrapper\">\n              <h3>Playoff Game Logs</h3>\n              <table>\n                <thead>\n                  <tr>\n                    <th>Date</th>\n                    <th>Matchup</th>\n                    <th>WL</th>\n                    <th>MIN</th>\n                    <th>FGM</th>\n                    <th>FGA</th>\n                    <th>FG%</th>\n                    <th>3PM</th>\n                    <th>3PA</th>\n                    <th>3P%</th>\n                    <th>FTM</th>\n                    <th>FTA</th>\n                    <th>FT%</th>\n                    <th>REB</th>\n                    <th>AST</th>\n                    <th>STL</th>\n                    <th>BLK</th>\n                    <th>PTS</th>\n                    <th>+/-</th>\n                  </tr>\n                </thead>\n                <tbody>\n                  <tr v-for=\"log in playoffLogs\" :key=\"log.game_id\">\n                    <td>{{ log.game_date }}</td>\n                    <td>{{ log.matchup }}</td>\n                    <td>{{ log.wl }}</td>\n                    <td>{{ log.min }}</td>\n                    <td>{{ log.fgm }}</td>\n                    <td>{{ log.fga }}</td>\n                    <td>{{ log.fg_pct }}</td>\n                    <td>{{ log.fg3m }}</td>\n                    <td>{{ log.fg3a }}</td>\n                    <td>{{ log.fg3_pct }}</td>\n                    <td>{{ log.ftm }}</td>\n                    <td>{{ log.fta }}</td>\n                    <td>{{ log.ft_pct }}</td>\n                    <td>{{ log.reb }}</td>\n                    <td>{{ log.ast }}</td>\n                    <td>{{ log.stl }}</td>\n                    <td>{{ log.blk }}</td>\n                    <td>{{ log.pts }}</td>\n                    <td>{{ log.plus_minus }}</td>\n                  </tr>\n                </tbody>\n              </table>\n            </div>\n          </div>\n        </div>\n      </main>\n    </div>\n\n    <footer>\n      <p>&copy; 2024 NBA Stats App</p>\n    </footer>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      searchQuery: '',\n      players: [],\n      selectedPlayer: null,\n      gameLogs: [],\n      playoffLogs: [],\n      isMenuOpen: false,\n      showDropdown: false\n    };\n  },\n  methods: {\n    toggleMenu() {\n      this.isMenuOpen = !this.isMenuOpen;\n    },\n    async searchPlayers() {\n      try {\n        if (this.searchQuery.trim() === '') {\n          this.players = [];\n          return;\n        }\n        console.log(`Searching for players with query: ${this.searchQuery}`);\n        const playerResponse = await axios.get(\n          `http://localhost:8000/api/players/${this.searchQuery}`\n        );\n        console.log('Player search response:', playerResponse.data);\n        this.players = playerResponse.data;\n        this.showDropdown = true;\n      } catch (error) {\n        console.error('Error fetching players:', error);\n        this.players = [];\n        this.showDropdown = false;\n      }\n    },\n    selectPlayer(player) {\n      this.selectedPlayer = player;\n      this.showDropdown = false;\n      this.fetchPlayerData(player.id);\n    },\n    async fetchPlayerData(playerId) {\n      try {\n        console.log(`Fetching game logs for player ID: ${playerId}`);\n        const gameLogsResponse = await axios.get(\n          `http://localhost:8000/api/players/${playerId}/games`\n        );\n        console.log('Game logs response:', gameLogsResponse.data);\n        this.gameLogs = gameLogsResponse.data;\n\n        console.log(`Fetching playoff logs for player ID: ${playerId}`);\n        const playoffLogsResponse = await axios.get(\n          `http://localhost:8000/api/players/${playerId}/playoffstats`\n        );\n        console.log('Playoff logs response:', playoffLogsResponse.data);\n        this.playoffLogs = playoffLogsResponse.data;\n      } catch (error) {\n        console.error('Error fetching game logs or playoff logs:', error);\n        this.gameLogs = [];\n        this.playoffLogs = [];\n      }\n    },\n    hideDropdown() {\n      setTimeout(() => {\n        this.showDropdown = false;\n      }, 200);\n    }\n  }\n};\n</script>\n\n<style>\n.app-container {\n  display: flex;\n  flex-direction: column;\n  min-height: 100vh;\n}\n\nheader {\n  background-color: #333;\n  color: white;\n  padding: 20px;\n  text-align: center;\n  position: relative;\n}\n\nheader .menu-button {\n  position: absolute;\n  left: 20px;\n  top: 20px;\n  background: none;\n  border: none;\n  color: white;\n  font-size: 24px;\n  cursor: pointer;\n}\n\nfooter {\n  background-color: #333;\n  color: white;\n  padding: 10px;\n  text-align: center;\n  margin-top: auto;\n}\n\n.content {\n  display: flex;\n  flex: 1;\n}\n\naside {\n  width: 200px;\n  background-color: #f4f4f4;\n  padding: 20px;\n  position: absolute;\n  left: -200px;\n  height: 100%;\n  transition: left 0.3s ease-in-out;\n}\n\naside.open {\n  left: 0;\n}\n\nnav ul {\n  list-style-type: none;\n  padding: 0;\n}\n\nnav ul li {\n  margin: 10px 0;\n  cursor: pointer;\n}\n\nmain {\n  flex: 1;\n  padding: 20px;\n  margin-left: 200px;\n}\n\n.player-search {\n  max-width: 800px;\n  margin: 0 auto;\n  text-align: center;\n  position: relative;\n}\n\nul {\n  list-style-type: none;\n  padding: 0;\n}\n\nli {\n  cursor: pointer;\n  margin: 5px 0;\n  padding: 10px;\n  background-color: #f4f4f4;\n  border: 1px solid #ddd;\n}\n\nli:hover {\n  background-color: #ddd;\n}\n\n.dropdown {\n  position: absolute;\n  background-color: white;\n  border: 1px solid #ddd;\n  width: 100%;\n  max-height: 200px;\n  overflow-y: auto;\n  z-index: 1000;\n}\n\n.table-wrapper {\n  margin-top: 20px;\n  overflow-x: auto;\n}\n\ntable {\n  width: 100%;\n  border-collapse: collapse;\n  margin-top: 10px;\n  font-size: 14px;\n}\n\nth, td {\n  border: 1px solid #ddd;\n  padding: 8px;\n  text-align: center;\n}\n\nth {\n  background-color: #f4f4f4;\n}\n\nthead th {\n  position: -webkit-sticky;\n  position: sticky;\n  top: 0;\n  z-index: 1;\n  background-color: #333;\n  color: white;\n}\n</style>\n"],"mappings"