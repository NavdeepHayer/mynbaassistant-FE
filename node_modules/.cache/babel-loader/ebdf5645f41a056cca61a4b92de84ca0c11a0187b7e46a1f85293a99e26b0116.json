{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'HomePage',\n  data() {\n    return {\n      topPlayers: [],\n      draftHistory: [],\n      draftYears: [],\n      selectedYear: ''\n    };\n  },\n  async created() {\n    this.fetchTopPlayers();\n    this.generateDraftYears();\n  },\n  methods: {\n    async fetchTopPlayers() {\n      try {\n        const response = await axios.get('http://localhost:8000/api/top10players');\n        this.topPlayers = response.data;\n      } catch (error) {\n        console.error('Error fetching top players:', error);\n      }\n    },\n    generateDraftYears() {\n      // Generate draft years from 2000 to the current year\n      const currentYear = new Date().getFullYear();\n      this.draftYears = Array.from({\n        length: currentYear - 1999\n      }, (_, i) => 2000 + i);\n      this.selectedYear = this.draftYears[0]; // Default to the first year\n    },\n    async fetchDraftHistory() {\n      if (!this.selectedYear) return;\n      try {\n        const response = await axios.get(`http://localhost:8000/api/drafthistory/${this.selectedYear}`);\n        this.draftHistory = response.data;\n      } catch (error) {\n        console.error('Error fetching draft history:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","topPlayers","draftHistory","draftYears","selectedYear","created","fetchTopPlayers","generateDraftYears","methods","response","get","error","console","currentYear","Date","getFullYear","Array","from","length","_","i","fetchDraftHistory"],"sources":["/home/nav/Desktop/NBA api/frontend/nba-stats-app/src/views/HomePage.vue"],"sourcesContent":["<template>\n  <div class=\"container mt-4\">\n    <h2>NBA Stats</h2>\n\n    <!-- Tabs -->\n    <ul class=\"nav nav-tabs\" id=\"statsTabs\" role=\"tablist\">\n      <li class=\"nav-item\" role=\"presentation\">\n        <button class=\"nav-link active\" id=\"top-players-tab\" data-bs-toggle=\"tab\" data-bs-target=\"#top-players\" type=\"button\" role=\"tab\" aria-controls=\"top-players\" aria-selected=\"true\">Top 10 Players</button>\n      </li>\n      <li class=\"nav-item\" role=\"presentation\">\n        <button class=\"nav-link\" id=\"draft-history-tab\" data-bs-toggle=\"tab\" data-bs-target=\"#draft-history\" type=\"button\" role=\"tab\" aria-controls=\"draft-history\" aria-selected=\"false\">Draft History</button>\n      </li>\n    </ul>\n\n    <!-- Tab Contents -->\n    <div class=\"tab-content mt-3\" id=\"statsTabsContent\">\n      <!-- Top Players Tab -->\n      <div class=\"tab-pane fade show active\" id=\"top-players\" role=\"tabpanel\" aria-labelledby=\"top-players-tab\">\n        <div class=\"table-responsive\">\n          <table class=\"table table-striped\">\n            <thead class=\"thead-dark\">\n              <tr>\n                <th>Name</th>\n                <th>Team</th>\n                <th>GP</th>\n                <th>PTS</th>\n                <th>REB</th>\n                <th>AST</th>\n                <th>STL</th>\n                <th>BLK</th>\n                <th>FG%</th>\n                <th>3P%</th>\n                <th>FT%</th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-for=\"player in topPlayers\" :key=\"player.player_id\">\n                <td>{{ player.player_name }}</td>\n                <td>{{ player.team }}</td>\n                <td>{{ player.gp }}</td>\n                <td>{{ player.pts }}</td>\n                <td>{{ player.reb }}</td>\n                <td>{{ player.ast }}</td>\n                <td>{{ player.stl }}</td>\n                <td>{{ player.blk }}</td>\n                <td>{{ player.fg_pct }}</td>\n                <td>{{ player.fg3_pct }}</td>\n                <td>{{ player.ft_pct }}</td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n      </div>\n\n      <!-- Draft History Tab -->\n      <div class=\"tab-pane fade\" id=\"draft-history\" role=\"tabpanel\" aria-labelledby=\"draft-history-tab\">\n        <div class=\"form-group\">\n          <label for=\"draftYear\">Select Draft Year:</label>\n          <select class=\"form-control\" id=\"draftYear\" v-model=\"selectedYear\" @change=\"fetchDraftHistory\">\n            <option v-for=\"year in draftYears\" :key=\"year\" :value=\"year\">{{ year }}</option>\n          </select>\n        </div>\n\n        <div class=\"table-responsive mt-3\" v-if=\"draftHistory.length\">\n          <table class=\"table table-striped\">\n            <thead class=\"thead-dark\">\n              <tr>\n                <th>Player Name</th>\n                <th>Team</th>\n                <th>Round</th>\n                <th>Round Pick</th>\n                <th>Overall Pick</th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-for=\"player in draftHistory\" :key=\"player.player_id\">\n                <td>{{ player.player_name }}</td>\n                <td>{{ player.team }}</td>\n                <td>{{ player.round_number }}</td>\n                <td>{{ player.round_pick }}</td>\n                <td>{{ player.overall_pick }}</td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'HomePage',\n  data() {\n    return {\n      topPlayers: [],\n      draftHistory: [],\n      draftYears: [],\n      selectedYear: '',\n    };\n  },\n  async created() {\n    this.fetchTopPlayers();\n    this.generateDraftYears();\n  },\n  methods: {\n    async fetchTopPlayers() {\n      try {\n        const response = await axios.get('http://localhost:8000/api/top10players');\n        this.topPlayers = response.data;\n      } catch (error) {\n        console.error('Error fetching top players:', error);\n      }\n    },\n    generateDraftYears() {\n      // Generate draft years from 2000 to the current year\n      const currentYear = new Date().getFullYear();\n      this.draftYears = Array.from({ length: currentYear - 1999 }, (_, i) => 2000 + i);\n      this.selectedYear = this.draftYears[0];  // Default to the first year\n    },\n    async fetchDraftHistory() {\n      if (!this.selectedYear) return;\n      try {\n        const response = await axios.get(`http://localhost:8000/api/drafthistory/${this.selectedYear}`);\n        this.draftHistory = response.data;\n      } catch (error) {\n        console.error('Error fetching draft history:', error);\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n.container {\n  padding: 20px;\n}\n\n.table-responsive {\n  margin-top: 20px;\n}\n\nthead.thead-dark {\n  background-color: #343a40;\n  color: white;\n}\n\n.nav-tabs .nav-link {\n  cursor: pointer;\n}\n\n.nav-tabs .nav-link.active {\n  background-color: #343a40;\n  color: white;\n}\n</style>\n"],"mappings":"AA2FA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,EAAE;MACdC,YAAY,EAAE,EAAE;MAChBC,UAAU,EAAE,EAAE;MACdC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI,CAACC,eAAe,CAAC,CAAC;IACtB,IAAI,CAACC,kBAAkB,CAAC,CAAC;EAC3B,CAAC;EACDC,OAAO,EAAE;IACP,MAAMF,eAAeA,CAAA,EAAG;MACtB,IAAI;QACF,MAAMG,QAAO,GAAI,MAAMX,KAAK,CAACY,GAAG,CAAC,wCAAwC,CAAC;QAC1E,IAAI,CAACT,UAAS,GAAIQ,QAAQ,CAACT,IAAI;MACjC,EAAE,OAAOW,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,6BAA6B,EAAEA,KAAK,CAAC;MACrD;IACF,CAAC;IACDJ,kBAAkBA,CAAA,EAAG;MACnB;MACA,MAAMM,WAAU,GAAI,IAAIC,IAAI,CAAC,CAAC,CAACC,WAAW,CAAC,CAAC;MAC5C,IAAI,CAACZ,UAAS,GAAIa,KAAK,CAACC,IAAI,CAAC;QAAEC,MAAM,EAAEL,WAAU,GAAI;MAAK,CAAC,EAAE,CAACM,CAAC,EAAEC,CAAC,KAAK,IAAG,GAAIA,CAAC,CAAC;MAChF,IAAI,CAAChB,YAAW,GAAI,IAAI,CAACD,UAAU,CAAC,CAAC,CAAC,EAAG;IAC3C,CAAC;IACD,MAAMkB,iBAAiBA,CAAA,EAAG;MACxB,IAAI,CAAC,IAAI,CAACjB,YAAY,EAAE;MACxB,IAAI;QACF,MAAMK,QAAO,GAAI,MAAMX,KAAK,CAACY,GAAG,CAAC,0CAA0C,IAAI,CAACN,YAAY,EAAE,CAAC;QAC/F,IAAI,CAACF,YAAW,GAAIO,QAAQ,CAACT,IAAI;MACnC,EAAE,OAAOW,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,+BAA+B,EAAEA,KAAK,CAAC;M