{"ast":null,"code":"import axios from 'axios';\nimport PlayerDetails from './PlayerDetails.vue';\nexport default {\n  name: 'PlayersTab',\n  components: {\n    PlayerDetails\n  },\n  data() {\n    return {\n      players: [],\n      searchQuery: '',\n      selectedPlayer: null,\n      page: 1,\n      perPage: 10,\n      totalPlayers: 0\n    };\n  },\n  computed: {\n    filteredPlayers() {\n      const start = (this.page - 1) * this.perPage;\n      const end = start + this.perPage;\n      return this.players.slice(start, end);\n    },\n    totalPages() {\n      return Math.ceil(this.totalPlayers / this.perPage);\n    }\n  },\n  methods: {\n    fetchPlayers() {\n      axios.get('/api/players').then(response => {\n        this.players = response.data;\n        this.totalPlayers = this.players.length;\n        if (this.players.length > 0) {\n          this.selectPlayer(this.players[0]);\n        }\n      }).catch(error => {\n        console.error('Error fetching players:', error);\n      });\n    },\n    searchPlayers() {\n      if (this.searchQuery.trim() === '') {\n        this.fetchPlayers();\n      } else {\n        axios.get(`/api/players/${this.searchQuery}`).then(response => {\n          this.players = response.data;\n          this.totalPlayers = this.players.length;\n          this.page = 1; // Reset to first page\n          if (this.players.length > 0) {\n            this.selectPlayer(this.players[0]);\n          }\n        }).catch(error => {\n          console.error('Error searching players:', error);\n        });\n      }\n    },\n    selectPlayer(player) {\n      this.selectedPlayer = player;\n    },\n    previousPage() {\n      if (this.page > 1) {\n        this.page--;\n      }\n    },\n    nextPage() {\n      if (this.page < this.totalPages) {\n        this.page++;\n      }\n    }\n  },\n  created() {\n    this.fetchPlayers();\n  }\n};","map":{"version":3,"names":["axios","PlayerDetails","name","components","data","players","searchQuery","selectedPlayer