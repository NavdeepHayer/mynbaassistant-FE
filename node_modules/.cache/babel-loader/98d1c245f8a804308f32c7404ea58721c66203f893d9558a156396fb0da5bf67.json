  <p class=\"card-text\">{{ team.conference }} - {{ team.division }}</p>\n              <button class=\"btn btn-primary\" @click=\"toggleCard(team.team_id)\">\n                View Players\n              </button>\n              <div v-if=\"isFlipped[team.team_id]\" class=\"player-list mt-3\">\n                <ul>\n                  <li v-for=\"player in selectedTeamPlayers[team.team_id]\" :key=\"player.id\" @click=\"selectPlayer(player)\">\n                    {{ player.full_name }}\n                  </li>\n                </ul>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <div v-if=\"activeTab === 'players'\" class=\"tab-content mt-4\">\n      <h2 class=\"text-center my-4\">NBA Players</h2>\n      <div class=\"player-search mt-4\">\n        <div class=\"input-group mb-3\">\n          <input\n            v-model=\"searchQuery\"\n            @input=\"handleSearchInput\"\n            @keyup.enter=\"searchPlayers\"\n            class=\"form-control\"\n            placeholder=\"Search for a player\"\n            @focus=\"showDropdown = true\"\n            @blur=\"hideDropdown\"\n          />\n          <div class=\"input-group-append\">\n            <button class=\"btn btn-outline-secondary\" @click=\"searchPlayers\">Search</button>\n          </div>\n        </div>\n\n        <div v-if=\"showDropdown && players.length\" class=\"dropdown-menu show w-100\">\n          <button\n            class=\"dropdown-item\"\n            v-for=\"player in players\"\n            :key=\"player.id\"\n            @mousedown=\"selectPlayer(player)\"\n          >\n            {{ player.full_name }}\n          </button>\n        </div>\n\n        <div class=\"row mt-4\">\n          <div v-for=\"player in players\" :key=\"player.id\" class=\"col-md-3 mb-4\">\n            <div class=\"card\" @click=\"selectPlayer(player)\">\n              <div class=\"card-body\">\n                <h5 class=\"card-title\">{{ player.full_name }