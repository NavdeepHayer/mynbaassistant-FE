{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'DraftTab',\n  data() {\n    return {\n      selectedYear: new Date().getFullYear(),\n      draftHistory: [],\n      years: Array.from({\n        length: 75\n      }, (_, i) => new Date().getFullYear() - i)\n    };\n  },\n  async created() {\n    await this.fetchDraftHistory();\n  },\n  methods: {\n    async fetchDraftHistory() {\n      try {\n        const response = await axios.get(`https://api.mynbaassistant.com/api/draft/${this.selectedYear}`, {\n          headers: {\n            Authorization: `Bearer ${localStorage.getItem('token')}`\n          }\n        });\n        this.draftHistory = response.data;\n      } catch (error) {\n        console.error(`Error fetching draft history for year ${this.selectedYear}:`, error);\n        this.draftHistory = [];\n      }\n    },\n    highlightClass(index) {\n      if (index === 0) {\n        return 'highlight-1';\n      } else if (index < 3) {\n        return 'highlight-2';\n      } else if (index < 10) {\n        return 'highlight-3';\n      } else if (index < 20) {\n        return 'highlight-4';\n      }\n      return '';\n    },\n    badgeClass(index) {\n      if (index === 0) {\n        return 'bg-gold';\n      } else if (index < 3) {\n        return 'bg-silver';\n      } else if (index < 10) {\n        return 'bg-bronze';\n      } else if (index < 20) {\n        return 'bg-blue';\n      }\n      return 'bg-secondary';\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","selectedYear","Date","getFullYear","draftHistory","years","Array","from","length","_","i","created","fetchDraftHistory","methods","response","get","headers","Authorization","localStorage","getItem","error","console","highlightClass","index","badgeClass"],"sources":["/home/nav/Documents/GitHub/mynbaassistant-FE/src/views/DraftTab.vue"],"sourcesContent":["<template>\n  <div class=\"draft-history container-fluid\">\n    <h2 class=\"text-center my-4\">Draft History</h2>\n    <div class=\"form-group row justify-content-center mb-4\">\n      <label for=\"yearSelect\" class=\"col-form-label col-auto\">Select Draft Year:</label>\n      <div class=\"col-auto\">\n        <select id=\"yearSelect\" class=\"form-select\" v-model=\"selectedYear\" @change=\"fetchDraftHistory\">\n          <option v-for=\"year in years\" :key=\"year\" :value=\"year\">{{ year }}</option>\n        </select>\n      </div>\n    </div>\n    <div v-if=\"draftHistory.length > 0\" class=\"list-group\">\n      <div\n        v-for=\"(record, index) in draftHistory\"\n        :key=\"record.id\"\n        :class=\"['list-group-item', 'list-group-item-dark', 'd-flex', 'justify-content-between', 'align-items-center', highlightClass(index)]\"\n      >\n        <div>\n          <h5 class=\"mb-1\">{{ record.player_name }}</h5>\n          <p class=\"mb-1\">{{ record.team_name }}</p>\n          <small>\n            <strong>Draft Year:</strong> {{ record.draft_year }} | \n            <strong>Round:</strong> {{ record.round }} | \n            <strong>Pick:</strong> {{ record.round_pick }} | \n            <strong>Overall:</strong> {{ record.overall_pick }}\n          </small>\n        </div>\n        <span class=\"badge\" :class=\"badgeClass(index)\">\n          {{ index + 1 }}\n        </span>\n      </div>\n    </div>\n    <div v-else class=\"text-center mt-4\">\n      <p>No data available for the selected year.</p>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'DraftTab',\n  data() {\n    return {\n      selectedYear: new Date().getFullYear(),\n      draftHistory: [],\n      years: Array.from({ length: 75 }, (_, i) => new Date().getFullYear() - i)\n    };\n  },\n  async created() {\n    await this.fetchDraftHistory();\n  },\n  methods: {\n    async fetchDraftHistory() {\n      try {\n        const response = await axios.get(`https://api.mynbaassistant.com/api/draft/${this.selectedYear}`, {\n          headers: {\n            Authorization: `Bearer ${localStorage.getItem('token')}`\n          }\n        });\n        this.draftHistory = response.data;\n      } catch (error) {\n        console.error(`Error fetching draft history for year ${this.selectedYear}:`, error);\n        this.draftHistory = [];\n      }\n    },\n    highlightClass(index) {\n      if (index === 0) {\n        return 'highlight-1';\n      } else if (index < 3) {\n        return 'highlight-2';\n      } else if (index < 10) {\n        return 'highlight-3';\n      } else if (index < 20) {\n        return 'highlight-4';\n      }\n      return '';\n    },\n    badgeClass(index) {\n      if (index === 0) {\n        return 'bg-gold';\n      } else if (index < 3) {\n        return 'bg-silver';\n      } else if (index < 10) {\n        return 'bg-bronze';\n      } else if (index < 20) {\n        return 'bg-blue';\n      }\n      return 'bg-secondary';\n    }\n  }\n};\n</script>\n\n<style scoped>\n.draft-history {\n  background-color: #121212; /* Darker background */\n  color: #e0e0e0; /* Lighter text color */\n  min-height: 100vh;\n}\n\n.form-select {\n  background-color: #2a2a2a;\n  color: #e0e0e0;\n  border: 1px solid #555;\n}\n\n.list-group-item {\n  background-color: #2a2a2a; /* Darker item background */\n  border: none;\n  transition: transform 0.3s ease, box-shadow 0.3s ease;\n  color: #e0e0e0; /* Lighter text color */\n}\n\n.list-group-item:hover {\n  transform: translateY(-3px);\n  box-shadow: 0 5px 10px rgba(0, 0, 0, 0.5);\n}\n\n.highlight-1 {\n  border-left: 5px solid #ffd700; /* Gold */\n  background: linear-gradient(90deg, rgba(255, 215, 0, 0.3), rgba(42, 42, 42, 0));\n}\n\n.highlight-2 {\n  border-left: 5px solid #c0c0c0; /* Silver */\n  background: linear-gradient(90deg, rgba(192, 192, 192, 0.3), rgba(42, 42, 42, 0));\n}\n\n.highlight-3 {\n  border-left: 5px solid #cd7f32; /* Bronze */\n  background: linear-gradient(90deg, rgba(205, 127, 50, 0.3), rgba(42, 42, 42, 0));\n}\n\n.highlight-4 {\n  border-left: 5px solid #007bff; /* Blue */\n  background: linear-gradient(90deg, rgba(0, 123, 255, 0.3), rgba(42, 42, 42, 0));\n}\n\n.bg-gold {\n  background-color: #ffd700 !important;\n  color: #000 !important;\n}\n\n.bg-silver {\n  background-color: #c0c0c0 !important;\n  color: #000 !important;\n}\n\n.bg-bronze {\n  background-color: #cd7f32 !important;\n  color: #000 !important;\n}\n\n.bg-blue {\n  background-color: #007bff !important;\n  color: #000 !important;\n}\n</style>\n"],"mappings":"AAuCA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,IAAIC,IAAI,CAAC,CAAC,CAACC,WAAW,CAAC,CAAC;MACtCC,YAAY,EAAE,EAAE;MAChBC,KAAK,EAAEC,KAAK,CAACC,IAAI,CAAC;QAAEC,MAAM,EAAE;MAAG,CAAC,EAAE,CAACC,CAAC,EAAEC,CAAC,KAAK,IAAIR,IAAI,CAAC,CAAC,CAACC,WAAW,CAAC,IAAIO,CAAC;IAC1E,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,MAAM,IAAI,CAACC,iBAAiB,CAAC,CAAC;EAChC,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,iBAAiBA,CAAA,EAAG;MACxB,IAAI;QACF,MAAME,QAAO,GAAI,MAAMhB,KAAK,CAACiB,GAAG,CAAC,4CAA4C,IAAI,CAACd,YAAY,EAAE,EAAE;UAChGe,OAAO,EAAE;YACPC,aAAa,EAAE,UAAUC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;UACxD;QACF,CAAC,CAAC;QACF,IAAI,CAACf,YAAW,GAAIU,QAAQ,CAACd,IAAI;MACnC,EAAE,OAAOoB,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,yCAAyC,IAAI,CAACnB,YAAY,GAAG,EAAEmB,KAAK,CAAC;QACnF,IAAI,CAAChB,YAAW,GAAI,EAAE;MACxB;IACF,CAAC;IACDkB,cAAcA,CAACC,KAAK,EAAE;MACpB,IAAIA,KAAI,KAAM,CAAC,EAAE;QACf,OAAO,aAAa;MACtB,OAAO,IAAIA,KAAI,GAAI,CAAC,EAAE;QACpB,OAAO,aAAa;MACtB,OAAO,IAAIA,KAAI,GAAI,EAAE,EAAE;QACrB,OAAO,aAAa;MACtB,OAAO,IAAIA,KAAI,GAAI,EAAE,EAAE;QACrB,OAAO,aAAa;MACtB;MACA,OAAO,EAAE;IACX,CAAC;IACDC,UAAUA,CAACD,KAAK,EAAE;MAChB,IAAIA,KAAI,KAAM,CAAC,EAAE;QACf,OAAO,SAAS;MAClB,OAAO,IAAIA,KAAI,GAAI,CAAC,EAAE;QACpB,OAAO,WAAW;MACpB,OAAO,IAAIA,KAAI,GAAI,EAAE,EAAE;QACrB,OAAO,WAAW;MACpB,OAAO,IAAIA,KAAI,GAAI,EAAE,EAAE;QACrB,OAAO,SAAS;MAClB;MACA,OAAO,cAAc;IACvB;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}