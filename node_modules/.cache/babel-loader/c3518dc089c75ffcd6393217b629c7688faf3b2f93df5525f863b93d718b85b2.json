     <th>MIN</th>\n                      <th>FGM</th>\n                      <th>FGA</th>\n                      <th>FG%</th>\n                      <th>3PM</th>\n                      <th>3PA</th>\n                      <th>3P%</th>\n                      <th>FTM</th>\n                      <th>FTA</th>\n                      <th>FT%</th>\n                      <th>REB</th>\n                      <th>AST</th>\n                      <th>STL</th>\n                      <th>BLK</th>\n                      <th>PTS</th>\n                      <th>+/-</th>\n                    </tr>\n                  </thead>\n                  <tbody>\n                    <tr v-for=\"log in filteredGameLogs\" :key=\"log.game_id\">\n                      <td>{{ log.game_date }}</td>\n                      <td>{{ log.matchup }}</td>\n                      <td>{{ log.wl }}</td>\n                      <td>{{ log.min }}</td>\n                      <td>{{ log.fgm }}</td>\n                      <td>{{ log.fga }}</td>\n                      <td>{{ log.fg_pct }}</td>\n                      <td>{{ log.fg3m }}</td>\n                      <td>{{ log.fg3a }}</td>\n                      <td>{{ log.fg3_pct }}</td>\n                      <td>{{ log.ftm }}</td>\n                      <td>{{ log.fta }}</td>\n                      <td>{{ log.ft_pct }}</td>\n                      <td>{{ log.reb }}</td>\n                      <td>{{ log.ast }}</td>\n                      <td>{{ log.stl }}</td>\n                      <td>{{ log.blk }}</td>\n                      <td>{{ log.pts }}</td>\n                      <td>{{ log.plus_minus }}</td>\n                    </tr>\n                  </tbody>\n                </table>\n              </div>\n            </div>\n            <div class=\"tab-pane fade\" id=\"season-stats\" role=\"tabpanel\" aria-labelledby=\"season-stats-tab\">\n              <div class=\"table-responsive\">\n                <h3>Season Stats</h3>\n                <table class=\"table table-striped\">\n                  <thead class=\"thead-dark\">\n                    <tr>\n                      <th>Season</th>\n                      <th>Team</th>\n                      <th>GP</th>\n                      <th>GS</th>\n                      <th>MIN</th>\n                      <th>FGM</th>\n                      <th>FGA</th>\n                      <th>FG%</th>\n                      <th>3PM</th>\n                      <th>3PA</th>\n                      <th>3P%</th>\n                      <th>FTM</th>\n                      <th>FTA</th>\n                      <th>FT%</th>\n                      <th>OREB</th>\n                      <th>DREB</th>\n                      <th>REB</th>\n                      <th>AST</th>\n                      <th>STL</th>\n                      <th>BLK</th>\n                      <th>TOV</th>\n                      <th>PF</th>\n                      <th>PTS</th>\n                    </tr>\n                  </thead>\n                  <tbody>\n                    <tr v-for=\"stat in filteredSeasonStats\" :key=\"stat.season_id\">\n                      <td>{{ stat.season_id }}</td>\n                      <td>{{ stat.team_id }}</td>\n                      <td>{{ stat.gp }}</td>\n                      <td>{{ stat.gs }}</td>\n                      <td>{{ stat.min }}</td>\n                      <td>{{ stat.fgm }}</td>\n                      <td>{{ stat.fga }}</td>\n                      <td>{{ stat.fg_pct }}</td>\n                      <td>{{ stat.fg3m }}</td>\n                      <td>{{ stat.fg3a }}</td>\n                      <td>{{ stat.fg3_pct }}</td>\n                      <td>{{ stat.ftm }}</td>\n                      <td>{{ stat.fta }}</td>\n                      <td>{{ stat.ft_pct }}</td>\n                      <td>{{ stat.oreb }}</td>\n                      <td>{{ stat.dreb }}</td>\n                      <td>{{ stat.reb }}</td>\n                      <td>{{ stat.ast }}</td>\n                      <td>{{ stat.stl }}</td>\n                      <td>{{ stat.blk }}</td>\n                      <td>{{ stat.tov }}</td>\n                      <td>{{ stat.pf }}</td>\n                      <td>{{ stat.pts }}</td>\n                    </tr>\n                  </tbody>\n                </table>\n              </div>\n            </div>\n            <div class=\"tab-pane fade\" id=\"playoffs\" role=\"tabpanel\" aria-labelledby=\"playoffs-tab\">\n              <div class=\"table-responsive\">\n                <h3>Playoff Stats</h3>\n                <table class=\"table table-striped\">\n                  <thead class=\"thead-dark\">\n                    <tr>\n                      <th>Season</th>\n                      <th>Team</th>\n                      <th>GP</th>\n                      <th>GS</th>\n                      <th>MIN</th>\n                      <th>FGM</th>\n                      <th>FGA</th>\n                      <th>FG%</th>\n                      <th>3PM</th>\n                      <th>3PA</th>\n                      <th>3P%</th>\n                      <th>FTM</th>\n                      <th>FTA</th>\n                      <th>FT%</th>\n                      <th>OREB</th>\n                      <th>DREB</th>\n                      <th>REB</th>\n                      <th>AST</th>\n                      <th>STL</th>\n                      <th>BLK</th>\n                      <th>TOV</th>\n                      <th>PF</th>\n                      <th>PTS</th>\n                    </tr>\n                  </thead>\n                  <tbody>\n                    <tr v-for=\"stat in filteredPlayoffStats\" :key=\"stat.season_id\">\n                      <td>{{ stat.season_id }}</td>\n                      <td>{{ stat.team_id }}</td>\n                      <td>{{ stat.gp }}</td>\n                      <td>{{ stat.gs }}</td>\n                      <td>{{ stat.min }}</td>\n                      <td>{{ stat.fgm }}</td>\n                      <td>{{ stat.fga }}</td>\n                      <td>{{ stat.fg_pct }}</td>\n                      <td>{{ stat.fg3m }}</td>\n                      <td>{{ stat.fg3a }}</td>\n                      <td>{{ stat.fg3_pct }}</td>\n                      <td>{{ stat.ftm }}</td>\n                      <td>{{ stat.fta }}</td>\n                      <td>{{ stat.ft_pct }}</td>\n                      <td>{{ stat.oreb }}</td>\n                      <td>{{ stat.dreb }}</td>\n                      <td>{{ stat.reb }}</td>\n                      <td>{{ stat.ast }}</td>\n                      <td>{{ stat.stl }}</td>\n                      <td>{{ stat.blk }}</td>\n                      <td>{{ stat.tov }}</td>\n                      <td>{{ stat.pf }}</td>\n                      <td>{{ stat.pts }}</td>\n                    </tr>\n                  </tbody>\n                </table>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <!-- Draft History Tab -->\n    <div v-if=\"activeTab === 'draft'\" class=\"tab-content mt-4\">\n      <h2 class=\"text-center my-4\">NBA Draft History</h2>\n      <div class=\"form-group\">\n        <label for=\"draftYear\">Select Draft Year:</label>\n        <select v-model=\"selectedDraftYear\" @change=\"fetchDraftHistory\" class=\"form-control\" id=\"draftYear\">\n          <option v-for=\"year in draftYears\" :key=\"year\" :value=\"year\">{{ year }}</option>\n        </select>\n      </div>\n      <div v-if=\"draftHistory.length > 0\" class=\"table-responsive mt-4\">\n        <h3>Draft History for {{ selectedDraftYear }}</h3>\n        <table class=\"table table-striped\">\n          <thead class=\"thead-dark\">\n            <tr>\n              <th>Player Name</th>\n              <th>Team</th>\n              <th>Round</th>\n              <th>Round Pick</th>\n              <th>Overall Pick</th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr v-for=\"player in draftHistory\" :key=\"player.player_id\">\n              <td>{{ player.player_name }}</td>\n              <td>{{ player.team_id }}</td>\n              <td>{{ player.round_number }}</td>\n              <td>{{ player.round_pick }}</td>\n              <td>{{ player.overall_pick }}</td>\n            </tr>\n          </tbody>\n        </table>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'HomePage',\n  data() {\n    return {\n      activeTab: 'teams',\n      teams: [],\n      players: [],\n      searchQuery: '',\n      showDropdown: false,\n      selectedPlayer: null,\n      gameLogs: [],\n      seasonStats: [],\n      playoffStats: [],\n      isFlipped: {},\n      selectedTeamPlayers: {},\n      draftYears: [],\n      selectedDraftYear: '',\n      draftHistory: []\n    };\n  },\n  async created() {\n    this.fetchTeams();\n    this.fetchPlayers();\n    this.fetchDraftYears();\n  },\n  methods: {\n    async fetchTeams() {\n      try {\n        const response = await axios.get('http://localhost:8000/api/teams');\n        this.teams = response.data;\n      } catch (error) {\n        console.error('Error fetching NBA teams:', error);\n      }\n    },\n    async fetchPlayers() {\n      try {\n        const response = await axios.get('http://localhost:8000/api/players');\n        this.players = response.data;\n      } catch (error) {\n        console.error('Error fetching NBA players:', error);\n      }\n    },\n    async fetchDraftYears() {\n      try {\n        const response = await axios.get('http://localhost:8000/api/seasons');\n        this.draftYears = response.data;\n      } catch (error) {\n        console.error('Error fetching draft years:', error);\n      }\n    },\n    async fetchDraftHistory() {\n      try {\n        const response = await axios.get(`http://localhost:8000/api/drafthistory/${this.selectedDraftYear}`);\n        this.draftHistory = response.data;\n      } catch (error) {\n        console.error('Error fetching draft history:', error);\n      }\n 