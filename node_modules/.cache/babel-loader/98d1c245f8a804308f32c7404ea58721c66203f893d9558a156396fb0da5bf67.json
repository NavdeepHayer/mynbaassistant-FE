{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'HomePage',\n  data() {\n    return {\n      activeTab: 'teams',\n      teams: [],\n      players: [],\n      searchQuery: '',\n      showDropdown: false,\n      selectedPlayer: null,\n      gameLogs: [],\n      seasonStats: [],\n      playoffStats: [],\n      isFlipped: {},\n      selectedTeamPlayers: {},\n      selectedYear: new Date().getFullYear(),\n      draftHistory: [],\n      years: Array.from({\n        length: 15\n      }, (_, i) => new Date().getFullYear() - i)\n    };\n  },\n  async created() {\n    this.fetchTeams();\n    this.fetchPlayers();\n    this.fetchDraftHistory();\n  },\n  methods: {\n    async fetchTeams() {\n      try {\n        const response = await axios.get('http://localhost:8000/api/teams');\n        this.teams = response.data;\n      } catch (error) {\n        console.error('Error fetching NBA teams:', error);\n      }\n    },\n    async fetchPlayers() {\n      try {\n        const response = await axios.get('http://localhost:8000/api/players');\n        this.players = response.data;\n      } catch (error) {\n        console.error('Error fetching NBA players:', error);\n      }\n    },\n    async fetchPlayersByTeam(teamId) {\n      try {\n        const response = await axios.get(`http://localhost:8000/api/teams/${teamId}/players`);\n        this.$set(this.selectedTeamPlayers, teamId, response.data);\n      } catch (error) {\n        console.error('Error fetching players for the team:', error);\n      }\n    },\n    async toggleCard(teamId) {\n      if (!this.selectedTeamPlayers[teamId]) {\n        await this.fetchPlayersByTeam(teamId);\n      }\n      this.$set(this.isFlipped, teamId, !this.isFlipped[teamId]);\n    },\n    handleSearchInput() {\n      if (this.searchQuery.trim() === '') {\n        this.players = [];\n      } else {\n        this.searchPlayers();\n      }\n    },\n    async searchPlayers() {\n      try {\n        const playerResponse = await axios.get(`http://localhost:8000/api/players/${this.searchQuery}`);\n        this.players = playerResponse.data;\n        this.showDropdown = true;\n      } catch (error) {\n        this.players = [];\n        this.showDropdown = false;\n      }\n    },\n    async selectPlayer(player) {\n      this.selectedPlayer = player;\n      this.showDropdown = false;\n      await this.fetchPlayerData(player.id);\n    },\n    async fetchPlayerData(playerId) {\n      try {\n        const gameLogsResponse = await axios.get(`http://localhost:8000/api/players/${playerId}/games`);\n        this.gameLogs = gameLogsResponse.data;\n        const seasonStatsResponse = await axios.get(`http://localhost:8000/api/players/${playerId}/seasonstats`);\n        this.seasonStats = seasonStatsResponse.data;\n        const playoffStatsResponse = await axios.get(`http://localhost:8000/api/players/${playerId}/playoffstats`);\n        this.playoffStats = playoffStatsResponse.data;\n      } catch (error) {\n        this.gameLogs = [];\n        this.seasonStats = [];\n        this.playoffStats = [];\n      }\n    },\n    async fetchDraftHistory() {\n      try {\n        const response = await axios.get(`http://localhost:8000/api/draft/${this.selectedYear}`);\n        this.draftHistory = response.data;\n      } catch (error) {\n        console.error(`Error fetching draft history for year ${this.selectedYear}:`, error);\n        this.draftHistory = [];\n      }\n    },\n    hideDropdown() {\n      setTimeout(() => {\n        this.showDropdown = false;\n      }, 200);\n    }\n  },\n  computed: {\n    filteredGameLogs() {\n      return this.gameLogs.filter(log => log.min != null);\n    },\n    filteredSeasonStats() {\n      return this.seasonStats.filter(stat => stat.min != null);\n    },\n    filteredPlayoffStats() {\n      return this.playoffStats.filter(stat => stat.min != null);\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","activeTab","teams","players","searchQuery","showDropdown","sele