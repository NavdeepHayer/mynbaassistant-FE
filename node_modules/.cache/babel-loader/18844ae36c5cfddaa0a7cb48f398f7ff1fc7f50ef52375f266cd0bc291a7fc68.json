{"ast":null,"code":"import axios from 'axios';\nexport default {\n  props: {\n    player: Object\n  },\n  data() {\n    return {\n      gameLogs: [],\n      seasonStats: [],\n      playoffStats: [],\n      seasonFilter: ''\n    };\n  },\n  watch: {\n    player: {\n      immediate: true,\n      handler(newPlayer) {\n        if (newPlayer) {\n          this.fetchGameLogs();\n          this.fetchSeasonStats();\n          this.fetchPlayoffStats();\n        }\n      }\n    }\n  },\n  methods: {\n    fetchGameLogs() {\n      if (this.player) {\n        axios.get(`/api/players/${this.player.id}/games`, {\n          headers: {\n            Authorization: `Bearer ${localStorage.getItem('token')}`\n          },\n          params: {\n            season: this.seasonFilter\n          }\n        }).then(response => {\n          this.gameLogs = response.data;\n        }).catch(error => {\n          console.error('Error fetching game logs:', error);\n        });\n      }\n    },\n    fetchSeasonStats() {\n      if (this.player) {\n        axios.get(`/api/players/${this.player.id}/seasonstats`, {\n          headers: {\n            Authorization: `Bearer ${localStorage.getItem('token')}`\n          }\n        }).then(response => {\n          this.seasonStats = response.data;\n        }).catch(error => {\n          console.error('Error fetching season stats:', error);\n        });\n      }\n    },\n    fetchPlayoffStats() {\n      if (this.player) {\n        axios.get(`/api/players/${this.player.id}/playoffstats`, {\n          headers: {\n            Authorization: `Bearer ${localStorage.getItem('token')}`\n          }\n        }).then(response => {\n          this.playoffStats = response.data;\n        }).catch(error => {\n          console.error('Error fetching playoff stats:', error);\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","props","player","Object","data","gameLogs","seasonStats","playoffStats","seasonFilter","watch","immediate","handler","newPla